    <div class="login-img" style="width:100%;height:100%;position: absolute;justify-content: flex-end;display:flex;align-items: end; overflow: hidden;">
        <!-- Inicio de un carrusel -->
        <div id="image-carousel" style="width: 100%; height: 100%; overflow: hidden; position: relative;">
            <img id="image-1" src="/moodle/theme/space/pix/fp/banner-login-01.png" alt="Image 1" style="width: 100%; height: 100%; object-fit: cover; position: absolute; transition: opacity 0.5s;">
            <img id="image-2" src="/moodle/theme/space/pix/fp/banner-login-02.png" alt="Image 2" style="width: 100%; height: 100%; object-fit: cover; position: absolute; transition: opacity 0.5s; opacity: 0;">
            <img id="image-3" src="/moodle/theme/space/pix/fp/banner-login-03.png" alt="Image 3" style="width: 100%; height: 100%; object-fit: cover; position: absolute; transition: opacity 0.5s; opacity: 0;">
        </div>
        <!-- Fin de un carrusel -->
    </div>
    
    <script>
    var imgIndex = 1; // Inicia con la primera imagen visible
    var totalImages = 3; // Total de imágenes en el carrusel
    
    function fixImagePosition() {
        for (var i = 1; i <= totalImages; i++) {
            var currentImage = document.getElementById(`image-${i}`);
            currentImage.style.transition = "none"; // Desactiva la transición para cambiar la posición
            currentImage.style.transform = "translateX(0)";
            void currentImage.offsetWidth; // Fuerza el reflow para aplicar inmediatamente la transformación
            //currentImage.style.transition = "opacity 0.5s"; // Reactiva la transición
    
            currentImage.style.opacity = i === imgIndex ? "1" : "0";
        }
    }
    
    function changeImage() {
        var currentImage = document.getElementById(`image-${imgIndex}`);
        var nextIndex = imgIndex === totalImages ? 1 : imgIndex + 1;
        var nextImage = document.getElementById(`image-${nextIndex}`);
    
        // Desactiva la transición para cambiar la opacidad inmediatamente
        currentImage.style.transition = "none";
        currentImage.style.opacity = "0";
        void currentImage.offsetWidth; // Fuerza el reflow para aplicar inmediatamente la opacidad
    
        // Reactiva la transición para el próximo cambio
        //currentImage.style.transition = "opacity 0.5s";
    
        imgIndex = nextIndex;
    
        fixImagePosition();
    }
    
    // Cambia la imagen cada 5 segundos (5000 milisegundos)
    setInterval(changeImage, 5000);
    </script>
    
    
